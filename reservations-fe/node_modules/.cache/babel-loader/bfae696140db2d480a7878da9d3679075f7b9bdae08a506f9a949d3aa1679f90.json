     </ul>\r\n      </div>\r\n    </transition>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Logo from \"@/components/layout/Logo.vue\";\r\nimport LoginButton from \"@/components/layout/LoginButton.vue\";\r\nimport vClickOutside from \"click-outside-vue3\";\r\n\r\nexport default {\r\n  components: {\r\n    Logo,\r\n    LoginButton\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      mobile: null,\r\n      mobileNav: null,\r\n      windowWidth: null,\r\n      entertainment: false,\r\n      service: false,\r\n      userinfo: false\r\n    };\r\n  },\r\n  created() {\r\n    window.addEventListener(\"resize\", this.checkScreen);\r\n    this.checkScreen();\r\n  },\r\n  methods: {\r\n    toggleMobileNav() {\r\n      this.closeEntertainment();\r\n      this.closeUserinfo();\r\n      this.closeService();\r\n      this.mobileNav = !this.mobileNav;\r\n    },\r\n    closeMobileNav() {\r\n      this.closeEntertainment();\r\n      this.closeUserinfo();\r\n      this.closeService();\r\n      this.mobileNav = false;\r\n    },\r\n    toggleEntertainment() {\r\n      this.closeUserinfo();\r\n      this.closeService();\r\n      this.entertainment = !this.entertainment;\r\n    },\r\n    toggleService() {\r\n      this.closeEntertainment();\r\n      this.closeUserinfo();\r\n      this.service = !this.service;\r\n    },\r\n    closeService() {\r\n      this.service = false;\r\n    },\r\n    closeEntertainment() {\r\n      this.entertainment = false;\r\n    },\r\n    toggleUserinfo() {\r\n      this.closeEntertainment();\r\n      this.closeService();\r\n      this.userinfo = !this.userinfo;\r\n    },\r\n    closeUserinfo() {\r\n      this.userinfo = false;\r\n    },\r\n    closeAll() {\r\n      this.closeEntertainment();\r\n      this.closeUserinfo();\r\n      this.closeService();\r\n    },\r\n    checkScreen() {\r\n      this.windowWidth = window.innerWidth;\r\n      if (this.windowWidth <= 1200) {\r\n        this.mobile = true;\r\n      } else {\r\n        this.mobile = false;