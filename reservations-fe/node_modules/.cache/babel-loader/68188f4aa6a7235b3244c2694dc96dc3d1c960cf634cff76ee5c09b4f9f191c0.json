{"ast":null,"code":"import img01 from '../../assets/images/img.png';\nimport img02 from '../../assets/images/img02.webp';\nimport img03 from '../../assets/images/img03.webp';\nimport img04 from '../../assets/images/img04.webp';\nimport img05 from '../../assets/images/img05.webp';\nexport default {\n  data() {\n    return {\n      currentImageIndex: 0,\n      maskState: '',\n      images: [img01, img02, img03, img04, img05]\n    };\n  },\n  methods: {\n    showMask() {\n      this.maskState = 'opaque';\n      setTimeout(() => {\n        this.maskState = 'transparent';\n      }, 1000);\n    },\n    startImageCarousel() {\n      setInterval(() => {\n        this.showMask();\n        setTimeout(() => {\n          this.currentImageIndex++;\n          if (this.currentImageIndex >= this.images.length) {\n            this.currentImageIndex = 0;\n          }\n        }, 1000);\n      }, 13000);\n    }\n  },\n  mounted() {\n    this.startImageCarousel();\n  },\n  created() {\n    this.images.forEach(image => {\n      const img = new Image();\n      img.src = image;\n    });\n  }\n};","map":{"version":3,"names":["img01","img02","img03","img04","img05","data","currentImageIndex","maskState","images","methods","showMask","setTimeout","startImageCarousel","setInterval","length","mounted","created","forEach","image","img","Image","src"],"sources":["E:\\ISQA 3900 - Web Application Development\\ISQA4900_Group3_Backend\\reservations-fe\\src\\components\\booking\\BookingCarousel.vue"],"sourcesContent":["<template>\r\n    <div class=\"background-image-container\">\r\n        <div\r\n            v-for=\"(image, index) in images\"\r\n            :key=\"index\"\r\n            class=\"background-image\"\r\n            :style=\"{backgroundImage: `url(${image})`}\"\r\n            v-show=\"currentImageIndex === index\">\r\n        </div>\r\n        <div v-if=\"$slots.title\" class=\"title\">\r\n          <slot name=\"title\"></slot>\r\n        </div>\r\n        <div class=\"content\">\r\n            <slot></slot>\r\n        </div>\r\n        <div class=\"black-mask\" :class=\"maskState\"></div>\r\n    </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport img01 from '../../assets/images/img.png'\r\nimport img02 from '../../assets/images/img02.webp'\r\nimport img03 from '../../assets/images/img03.webp'\r\nimport img04 from '../../assets/images/img04.webp'\r\nimport img05 from '../../assets/images/img05.webp'\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            currentImageIndex: 0,\r\n            maskState: '',\r\n            images: [\r\n              img01,\r\n              img02,\r\n              img03,\r\n              img04,\r\n              img05\r\n            ]\r\n        };\r\n    },\r\n    methods: {\r\n        showMask() {\r\n            this.maskState = 'opaque';\r\n            setTimeout(() => {\r\n                this.maskState = 'transparent';\r\n            }, 1000);\r\n        },\r\n        startImageCarousel() {\r\n            setInterval(() => {\r\n                this.showMask();\r\n                setTimeout(() => {\r\n                    this.currentImageIndex++;\r\n                    if (this.currentImageIndex >= this.images.length) {\r\n                        this.currentImageIndex = 0;\r\n                    }\r\n                }, 1000);\r\n            }, 13000);\r\n        }\r\n    },\r\n    mounted() {\r\n        this.startImageCarousel();\r\n    },\r\n    created() {\r\n        this.images.forEach((image) => {\r\n            const img = new Image();\r\n            img.src = image;\r\n        });\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n\r\n@import url('https://fonts.googleapis.com/css?family=Montserrat:100,200,300,400,500,600,700,800,900');\r\n\r\n.background-image-container {\r\n    position: relative;\r\n    width: 100%;\r\n    min-height: 100vh;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    overflow: auto;\r\n}\r\n\r\n.content {\r\n    z-index: 10;\r\n    position: relative;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    text-align: center;\r\n    padding: 1em;\r\n    box-sizing: border-box;\r\n    width: 100%;\r\n}\r\n\r\n.title {\r\n    cursor: default;\r\n    color: white;\r\n    z-index: 10;\r\n    position: absolute;\r\n    top: 80px;\r\n    width: 100%;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    text-align: center;\r\n    padding: 1em;\r\n    box-sizing: border-box;\r\n    opacity: 70%;\r\n}\r\n\r\n.background-image {\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    background-size: cover;\r\n    background-position: center;\r\n}\r\n\r\n.black-mask {\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    background-color: rgba(0, 0, 0, 0.4);\r\n    transition: background-color 1.0s ease-in-out;\r\n}\r\n\r\n.black-mask.opaque {\r\n    background-color: rgba(0, 0, 0, 1);\r\n}\r\n\r\n.black-mask.transparent {\r\n    background-color: rgba(0, 0, 0, 0.4);\r\n}\r\n\r\n@media (max-width: 920px) {\r\n    .title {\r\n        display: none;\r\n    }\r\n}\r\n</style>"],"mappings":"AAqBA,OAAOA,KAAI,MAAO,6BAA4B;AAC9C,OAAOC,KAAI,MAAO,gCAA+B;AACjD,OAAOC,KAAI,MAAO,gCAA+B;AACjD,OAAOC,KAAI,MAAO,gCAA+B;AACjD,OAAOC,KAAI,MAAO,gCAA+B;AAEjD,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,iBAAiB,EAAE,CAAC;MACpBC,SAAS,EAAE,EAAE;MACbC,MAAM,EAAE,CACNR,KAAK,EACLC,KAAK,EACLC,KAAK,EACLC,KAAK,EACLC,KAAI;IAEV,CAAC;EACL,CAAC;EACDK,OAAO,EAAE;IACLC,QAAQA,CAAA,EAAG;MACP,IAAI,CAACH,SAAQ,GAAI,QAAQ;MACzBI,UAAU,CAAC,MAAM;QACb,IAAI,CAACJ,SAAQ,GAAI,aAAa;MAClC,CAAC,EAAE,IAAI,CAAC;IACZ,CAAC;IACDK,kBAAkBA,CAAA,EAAG;MACjBC,WAAW,CAAC,MAAM;QACd,IAAI,CAACH,QAAQ,CAAC,CAAC;QACfC,UAAU,CAAC,MAAM;UACb,IAAI,CAACL,iBAAiB,EAAE;UACxB,IAAI,IAAI,CAACA,iBAAgB,IAAK,IAAI,CAACE,MAAM,CAACM,MAAM,EAAE;YAC9C,IAAI,CAACR,iBAAgB,GAAI,CAAC;UAC9B;QACJ,CAAC,